<%= render 'books/list', user: @user, book: @book %>

<h1>Users</h1>
<table>
  <tr>
    <th>image</th>
    <th>name</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= attachment_image_tag user, :profile_image, :fill, 300, 300, format: 'jpeg' %></td>
      <td><%= user.name %></td>
      <td><%= link_to 'show', user_path(user) %></td>
    </tr>
  <% end %>
</table>